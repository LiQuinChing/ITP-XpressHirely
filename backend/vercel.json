{
"installCommand": "npm install --legacy-peer-deps",
"version": 2,
"functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "includeFiles": [
        "routes/**",
        "controllers/**",
        "models/**",
        "utils/**",
        "*.js"
      ]
    }
},
"routes": [
    { "src": "^/(.*)$", "dest": "/api/index.js" }
  ]
}