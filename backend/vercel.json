{
"installCommand": "npm install --legacy-peer-deps",
"version": 2,
"functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "includeFiles": [
        "routes/**",
        "controllers/**",
        "models/**",
        "utils/**",
        "*.js"
      ]
    }
},
"builds": [
{
    "src": "index.js",
    "use": "@vercel/node"
},
{
    "src": "src/**/*",
    "use": "@vercel/static"
}
],
"routes":[
{ "src": "/(.*)", "dest": "/"}
]
}