{
"installCommand": "npm install --legacy-peer-deps",
"version": 2,
"functions": {
    "backend/index.js": {
      "runtime": "nodejs20.x",
      "includeFiles": [
        "backend/routes/**",
        "backend/controllers/**",
        "backend/models/**",
        "backend/utils/**",
        "backend/*.js"
      ]
    },
"builds": [
{
    "src": "index.js",
    "use": "@vercel/node"
},
{
    "src": "src/**/*",
    "use": "@vercel/static"
}
],
"routes":[
{ "src": "/(.*)", "dest": "/"}
]
}
}